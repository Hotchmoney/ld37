}

if (distance_to_point(obj_platform_jumper.x,obj_platform_jumper.y) &lt;= 0.001){
    y+=10;
}

hsp = move * movesp;
if (vsp &lt; 10) vsp += grav;

if (place_meeting(x,y+1,obj_platform_jumper))
{
    vsp = key_jump * -jumpsp;
    image_index = 0;
}
else
{
    image_index = 1;
}
if ((vsp &lt; 0) &amp;&amp; (!key_jump_held)) {vsp = max(vsp,0);}

if (obj_jumper_manager.lag==true and random_range(0,100) &lt;= 5){vsp = (abs(vsp));}

if (place_meeting(x+hsp,y,obj_platform_jumper))
{
    while (!place_meeting(x+sign(hsp),y+1,obj_platform_jumper))
    {
        x += sign(hsp);
    }
    hsp = 0;
}
x += hsp;

if (place_meeting(x,y+vsp,obj_platform_jumper))
{
    while (!place_meeting(x,y+sign(vsp),obj_platform_jumper))
    {
        y += sign(vsp);
    }
    vsp = 0;
}
y += vsp;

if (place_meeting(x,y,obj_platform_jumper)) y+=100;
